{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "neptune",
  "title": "neptune",
  "description": "Interface for neptune in raycast",
  "icon": "command-icon.png",
  "author": "matt",
  "license": "MIT",
  "commands": [
    {
      "name": "create-school-note",
      "title": "Create School Note",
      "description": "Use neptune to create a new school note document",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.48.9",
    "execa": "^7.1.1",
    "toml": "^3.0.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
